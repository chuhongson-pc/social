      </section>

    <div class="footer">
			<div class="footer-below">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                          <a>Copyright © 2015 by Social</a>
                          <%= image_tag("local1.png", :alt => "") %>
                        </div>
                    </div>
                </div>
            </div>
		</div>
  </body>
</html>